{
    "manifest_version": 3,
    "name": "Text Search",
    "version": "1.0",
    "description": "Search text using Text index",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ]
}