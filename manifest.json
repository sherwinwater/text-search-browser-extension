{
    "manifest_version": 3,
    "name": "Text Search Extension",
    "version": "1.0",
    "description": "Search text using Text index",
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ]
}